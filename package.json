    "prismic.io": "^3.1.3",
    "metalsmith-react-tpl": "^0.2.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.11.1",
    "publish": "^0.5.0",
    "react": "^15.3.0",
    "react-dom": "^15.2.1",
    "underscore": "^1.8.3",
    "babel-cli": "6.7.7",
